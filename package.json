{
  "name": "my-house-video-player",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "typeorm": "tsed typeorm",
    "add-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "update-database": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "build-assets": "node build-assets.js",
    "initial" : "node initial.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@tsed/ajv": "5.52.3",
    "@tsed/common": "5.52.3",
    "@tsed/core": "5.52.3",
    "@tsed/di": "5.52.3",
    "@tsed/socketio": "5.52.3",
    "@tsed/swagger": "5.52.3",
    "@tsed/typeorm": "5.52.3",
    "@types/swagger-schema-official": "2.0.20",
    "ajv": "6.12.2",
    "body-parser": "1.19.0",
    "bootbox": "^5.4.0",
    "bootstrap": "^4.4.1",
    "compression": "1.7.4",
    "concurrently": "5.2.0",
    "consolidate": "^0.15.1",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "ejs": "^3.1.2",
    "express": "4.17.1",
    "fs-extra": "^9.0.0",
    "jquery": "^3.5.1",
    "jquery-serializetojson": "^1.4.1",
    "method-override": "3.0.0",
    "sqlite3": "4.2.0",
    "srt-to-vtt": "^1.1.3",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "@tsed/cli-plugin-typeorm": "1.12.3",
    "@types/compression": "1.7.0",
    "@types/consolidate": "^0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/fs-extra": "^8.1.0",
    "@types/method-override": "0.0.31",
    "@types/node": "13.13.5",
    "concurrently": "5.2.0",
    "nodemon": "2.0.3",
    "ts-node": "8.10.1",
    "typescript": "3.8.3"
  }
}
